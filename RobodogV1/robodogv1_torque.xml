<mujoco model="RobodogV1 - torque control">
  <option iterations="50" timestep="0.005" solver="PGS" gravity="0 0 -9.81"/>
  <compiler angle="radian" meshdir="meshes" eulerseq="zyx" autolimits="true"/>

  <default>
    <joint limited="true" damping="2.0" armature="0.1" frictionloss="0.1"/>
    <geom condim="4" contype="1" conaffinity="15"
          friction="3.0 0.1 0.01"
          solref="0.02 1"
          solimp="0.9 0.95 0.001"
          margin="0.001"/>
    <motor ctrllimited="true" ctrlrange="-1 1"/>
    <default class="visualgeom">
      <geom material="visualgeom" condim="1" contype="0" conaffinity="0"/>
    </default>
  </default>

  <asset>
    <mesh name="base_link" file="base_link.STL"/>
    <mesh name="front_right_joint" file="front_right_joint.STL"/>
    <mesh name="front_right_hip" file="front_right_hip.STL"/>
    <mesh name="front_right_leg" file="front_right_leg.STL"/>
    <mesh name="front_left_joint" file="front_left_joint.STL"/>
    <mesh name="front_left_hip" file="front_left_hip.STL"/>
    <mesh name="front_left_leg" file="front_left_leg.STL"/>
    <mesh name="rear_right_joint" file="rear_right_joint.STL"/>
