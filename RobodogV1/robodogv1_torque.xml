    <actuatorpos name="rearRightHip_p" actuator="rearRightHip"/>
    <actuatorvel name="rearRightHip_v" actuator="rearRightHip"/>
    <actuatorfrc name="rearRightHip_f" actuator="rearRightHip" noise="0.001"/>
    <actuatorpos name="rearRightLeg_p" actuator="rearRightLeg"/>
    <actuatorvel name="rearRightLeg_v" actuator="rearRightLeg"/>
    <actuatorfrc name="rearRightLeg_f" actuator="rearRightLeg" noise="0.001"/>
    <actuatorpos name="rearLeftJoint_p" actuator="rearLeftJoint"/>
    <actuatorvel name="rearLeftJoint_v" actuator="rearLeftJoint"/>
