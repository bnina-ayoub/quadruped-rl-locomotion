            <site name="RR" pos="0 0 -0.02" size="0.01" type="sphere"/>
          </body>
        </body>
      </body>

      <body name="rear_left_joint_body" pos="-0.096651 0.044502 -0.016137" quat="0.707105 -0.707108 0 0">
        <inertial pos="-0.025267 0.0097183 0.0051161" quat="0.521752 0.475385 -0.524187 0.476458" mass="0.05551" diaginertia="1.77441e-05 1.16509e-05 9.16634e-06"/>
